{
  "name": "jpglens-website",
  "version": "1.1.0",
  "type": "module",
  "description": "Official website for jpglens - Universal AI-Powered UI Testing",
  "scripts": {
    "dev": "vite",
    "build": "node scripts/sync-docs.js && vite build && node copy-dirs.js && node add-cache-busting.js",
    "sync": "node scripts/sync-docs.js",
    "preview": "vite preview",
    "serve": "python -m http.server 8080",
    "test": "playwright test",
    "test:footer": "playwright test footer-analysis.spec.js",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --debug",
    "report": "playwright show-report",
    "deploy": "npm run build && vercel --prod"
  },
  "devDependencies": {
    "@playwright/test": "^1.54.2",
    "@vitejs/plugin-react": "^4.0.0",
    "terser": "^5.43.1",
    "vite": "^5.0.0"
  },
  "dependencies": {
    "highlight.js": "^11.9.0",
    "jpglens": "^1.1.0",
    "playwright": "^1.54.2",
    "prismjs": "^1.29.0"
  },
  "main": "index.js",
  "directories": {
    "doc": "docs",
    "example": "examples"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
